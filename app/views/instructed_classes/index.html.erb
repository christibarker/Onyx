<div class="header">
	<h1>Class Schedule</h1>
	 	<div class="row">
	 	<div class="col l3 s3"></div>
	 	 <div class="col l6 s9">
	 	 	<div class="card blue-grey darken-1">
	      <div class="card-content white-text">
	      	<%= link_to "Login to start a class", user_session_path %>
	   		</div>
			</div>
		</div>
	</div>
</div>

	<div class="row">
		<div class="col s3 m3"></div>
	  <div class="col s12 m6">
	    <div class="card blue-grey darken-1">
	      <div class="card-content white-text">
	        <span class="card-title">Create Schedule</span>
	    		<%= render 'form' %>
	      </div>
	    </div>
	  </div>
	  <div class="col s3 m3"></div>
	</div>

	<br>
	<table class="highlight responsive-table">
	  <thead>
	    <tr>
	        <th>Start Time</th>
	        <th>Sign up</th>
	        <th>Classes</th>
	        <th>Teacher</th>
	        <th>Duration</th>
	    </tr>
	  </thead>
	  <tbody>
	    <%= render @instructed_classes %>
	  </tbody>
	</table>
<br>